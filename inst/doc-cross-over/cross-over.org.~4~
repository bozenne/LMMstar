0.0407  4 0 0
0.041   0 4 0
0.1739  4 0 0
total   1 0 0
#+end_example

#+LaTeX: \end{minipage}
#+LaTeX: \begin{minipage}{0.3\linewidth} 
#+BEGIN_SRC R :exports results :results output :session *R* :cache no
setNames(list(rbind(vecP4[[2]],
           total = round(tapply(P4[2,],bloodpressureL$treatment,sum),3))), names(vecP4[2]))
#+END_SRC

#+RESULTS:
